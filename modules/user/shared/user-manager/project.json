{
  "name": "user-shared-user-manager",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/user/shared/user-manager/src",
  "projectType": "library",
  "tags": ["module:shared", "scope:user"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "modules/user/shared/user-manager/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "modules/user/shared/user-manager/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
